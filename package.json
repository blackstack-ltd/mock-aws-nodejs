package-lock.json{
  "name": "mock-aws-nodejs",
  "version": "1.0.0",
  "description": "A mock library for AWS SDK V3 to test code that depends on AWS services",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:integration": "npm run build && node --test integration_tests/**/*.integration.test.mjs",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "aws",
    "aws-sdk",
    "mock",
    "testing",
    "s3"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.943.0",
    "@aws-sdk/client-sts": "^3.943.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "@aws-sdk/client-s3": "^3.0.0"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18.0.0"
  }
}
